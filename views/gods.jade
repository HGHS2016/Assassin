doctype html
html(lang="en")
  head
    title= title
  
    link(rel="stylesheet", href="stylesheets/bootstrap.css", type="text/css")
    link(rel="stylesheet", href="stylesheets/style.css", type="text/css")
      
  body
    div(class="container")
      h1 Players
      table(id="player_table")
        tr()
          th(class="table-header") Name
          th(class="table-header") Role 
          th(class="table-header") Status
    div(class="container")      
      h1 Teams
      table(id="team_table")
        tr()
          th(class="table-header") TeamID 
          th(class="table-header") Player1 
          th(class="table-header") Player2 
          th(class="table-header") Target 

    script(src="js/jquery.js", type="text/javascript")
    script(src="js/playerlist.js", type="text/javascript")
    // script(src="js/teamlist.js", type="text/javascript")